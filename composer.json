{
    "name": "webvolant/abadmin",
    "description": "AdminPanel for Laravel 5",
    "keywords": [
        "webvolant",
        "abadmin"
    ],
    "homepage": "https://github.com/webvolant/abadmin",
    "license": "MIT",
    "authors": [
        {
            "name": "absolutions",
            "email": "barkalov_anton@mail.ru",
            "homepage": "absolutions.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : ">=5.3.0",
        "laravelcollective/html": "~5.0",
        "spatie/laravel-backup": "master",
        "intervention/image": "2.*",
        "pqb/filemanager-laravel": "master",
        "mcamara/laravel-localization": "1.0.*",
        "baum/baum": "~1.1"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "webvolant\\abadmin\\": "src/",
            "webvolant\\abadmin\\Http\\Middleware\\": "src/Http/Middleware/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "webvolant\\abadmin\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "master": "1.0"
        }
    }
}
